{
  "name": "telejson",
  "version": "6.0.4",
  "description": "",
  "keywords": [
    "JSON",
    "cyclic",
    "cyclical",
    "date",
    "function",
    "parse",
    "regex",
    "stringify"
  ],
  "homepage": "https://github.com/storybookjs/telejson",
  "bugs": {
    "url": "https://github.com/storybookjs/telejson/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/storybookjs/telejson.git"
  },
  "license": "MIT",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint src --ext .js,.ts",
    "prepublish": "yarn build",
    "test": "yarn test-node && yarn test-browser",
    "test-browser": "TZ=UTC jest --env=jsdom ./common ./browser",
    "test-node": "TZ=UTC jest --env=node ./common ./node"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": [
      "@storybook/eslint-config-storybook"
    ],
    "ignorePatterns": [
      "dist/*"
    ]
  },
  "jest": {},
  "dependencies": {
    "@types/is-function": "^1.0.0",
    "global": "^4.4.0",
    "is-function": "^1.0.2",
    "is-regex": "^1.1.2",
    "is-symbol": "^1.0.3",
    "isobject": "^4.0.0",
    "memoizerific": "^1.11.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@storybook/eslint-config-storybook": "^3.0.0",
    "@swc/core": "^1.2.179",
    "@types/jest": "^26.0.22",
    "@types/lodash.get": "^4.4.7",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "common-tags": "^1.8.0",
    "eslint": "^7.23.0",
    "jest": "^26.6.3",
    "lodash.get": "^4.4.2",
    "prettier": "^2.2.1",
    "regenerator-runtime": "^0.13.7",
    "tsup": "^5.12.7",
    "typescript": "^4.2.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "target": "es5",
    "format": [
      "esm",
      "cjs"
    ],
    "sourcemap": true,
    "clean": true,
    "dts": true
  }
}
